{
    "version": 2,
    "builds": [
        {
            "src": "api/proxy.js",
            "use": "@vercel/node"
        }
    ],
    "redirects": [
        {
            "source": "/api/proxy/(.*)",
            "destination": "/api/proxy.js"
        },
        {
            "source": "/proxy/:match*",
            "destination": "https://api.notion.com/v1/:match*"
        }
    ]
}
